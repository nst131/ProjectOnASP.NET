@{
    ViewBag.Title = "CreateAlbum";
}

@model PresentationYandexMusic.Areas.Admin.ViewModel.AlbumViewModel

<form action="/Admin/Admin/CreateAlbum" data-ajax="true" data-ajax-update="#result" method="post" enctype="multipart/form-data">
    <div id="result">
        @{Html.RenderPartial("FormAlbum", Model);}
    </div>
</form>

<link rel="stylesheet" href="~/Content/ColorValidation.css" />
<script src="~/scripts/SolutionAjaxForm.js"></script>

@section scripts{
    @Scripts.Render("~/scripts/jquery-3.4.1.min.js")
    @Scripts.Render("~/scripts/jquery.unobtrusive-ajax.js")
}

