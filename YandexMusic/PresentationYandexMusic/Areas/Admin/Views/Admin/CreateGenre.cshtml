@{
    ViewBag.Title = "FormGenre";
}

@model PresentationYandexMusic.Areas.Admin.ViewModel.GenreViewModel

<form action="/Admin/Admin/CreateGenre" data-ajax="true" data-ajax-update="#result" method="post" enctype="multipart/form-data">
    <div id="result">
        @{Html.RenderPartial("FormGenre", Model);}
    </div>
</form>

<link rel="stylesheet" href="~/Content/ColorValidation.css"/>
<script src="~/scripts/SolutionAjaxForm.js"></script>

@section scripts{
    @Scripts.Render("~/scripts/jquery-3.4.1.min.js")
    @Scripts.Render("~/scripts/jquery.unobtrusive-ajax.js")
}

